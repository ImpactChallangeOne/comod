{
  "manifest_version": 2,

  "name": "Comod",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",
  "background": {"page: popup.html"},

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "permissions": [
   "activeTab"
   ],

   "content_scripts": [ {
    "js": [ "jquery-3.1.1.js", "popup.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
  }